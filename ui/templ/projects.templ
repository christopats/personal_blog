package templ

import "time"

type Project struct {
	Slug        string
	Title       string
	Description string
	Tags        []string
	ImagePath   string
	Content     string
}

templ Projects(posts []BlogPost) {
	<div class="">
		<h1 class="text-3xl font-bold my-8">Blog Posts</h1>
		<div class="grid_col_flex">
			for _, post := range posts {
				@BlogPostCard(post)
			}
		</div>
	</div>
}
